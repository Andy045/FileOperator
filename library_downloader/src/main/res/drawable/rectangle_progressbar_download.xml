<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0dip" />
            <gradient
                android:angle="270"
                android:centerColor="#B1C1D8"
                android:centerY="0.75"
                android:endColor="#B1C1D8"
                android:startColor="#B1C1D8" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="0dip" />
                <gradient
                    android:angle="270"
                    android:centerColor="#B1C1D8"
                    android:centerY="0.75"
                    android:endColor="#B1C1D8"
                    android:startColor="#B1C1D8" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0dip" />
                <gradient
                    android:angle="270"
                    android:centerColor="@color/font_file_blue"
                    android:centerY="0.75"
                    android:endColor="@color/font_file_blue"
                    android:startColor="@color/font_file_blue" />
            </shape>
        </clip>
    </item>
</layer-list>